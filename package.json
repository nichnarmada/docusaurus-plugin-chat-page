{
  "name": "docusaurus-plugin-chat-page",
  "version": "0.1.4",
  "description": "A Docusaurus plugin for adding an AI-powered chat interface to your documentation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy-css",
    "watch": "tsc --watch",
    "clean": "rm -rf lib",
    "copy-css": "cp src/theme/ChatPage/styles.module.css lib/theme/ChatPage/"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@docusaurus/utils": "^3.7.0",
    "@langchain/core": "^0.3.40",
    "gray-matter": "^4.0.3",
    "langchain": "^0.3.19",
    "react-markdown": "^10.0.0",
    "remark": "^15.0.1",
    "strip-markdown": "^6.0.0"
  },
  "peerDependencies": {
    "@docusaurus/core": ">=2.0.0",
    "@google/generative-ai": "^0.2.1",
    "@langchain/anthropic": "^0.3.13",
    "@langchain/google-genai": "^0.1.8",
    "@langchain/openai": "^0.4.4",
    "@langchain/xai": "^0.0.2",
    "@langchain/pinecone": "^0.1.3",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "@langchain/openai": {
      "optional": true
    },
    "@langchain/anthropic": {
      "optional": true
    },
    "@langchain/google-genai": {
      "optional": true
